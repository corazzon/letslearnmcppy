{
	"servers": {
		"fastmcp_basic": {
			"type": "stdio",
			"command": "uv",
			"args": [
				"run",
				"--directory",
				"${workspaceFolder}",
				"python",
				"examples/fastmcp_basic/server.py"
			],
			"cwd": "${workspaceFolder}"
		},
		"fileSystem": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"mcp-server-filesystem",
				"${workspaceFolder}"
			]
		},
		"github": {
			"url": "https://api.githubcopilot.com/mcp/",
			"type": "http"
		}
	}
}